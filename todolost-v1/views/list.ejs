<%- include('header') -%>
        <div id="heading" class="box">
            <h1>
                <%= listTitle %>
            </h1>
        </div>

        <div class="box">
            <ul>
                <% for(var i=0;i<newListItems.length;i++){ %>
                    <div class="item">
                        <input type="checkbox">
                        <p>
                            <%= newListItems[i] %>
                        </p>
                    </div>
                    <% } %>

            </ul>

            <form action="/" method="post">
                <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
                <button type="submit" name="list" value=<%= listTitle%>>+</button>
            </form>
        </div>
<%- include('footer')-%>